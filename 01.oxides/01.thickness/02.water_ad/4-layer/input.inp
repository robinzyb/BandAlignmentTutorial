&GLOBAL
  PRINT_LEVEL medium
  PROJECT_NAME SnO2
  RUN_TYPE GEO_OPT
  WALLTIME 86000
&END GLOBAL
&MOTION
  &CELL_OPT
    KEEP_ANGLES .TRUE.
  &END CELL_OPT
&END MOTION
&FORCE_EVAL
  METHOD Quickstep
  STRESS_TENSOR ANALYTICAL
  &DFT
    POTENTIAL_FILE_NAME GTH_POTENTIALS
    BASIS_SET_FILE_NAME BASIS_MOLOPT
    &SCF
      MAX_SCF 50
      EPS_SCF 1.0E-6
      &OT
        MINIMIZER DIIS
        PRECONDITIONER FULL_SINGLE_INVERSE
        ENERGY_GAP 0.1
      &END OT
      &OUTER_SCF
        EPS_SCF 1.0E-6
        MAX_SCF 10
      &END OUTER_SCF
    &END SCF
    &QS
      EPS_DEFAULT 1.0E-13
      EXTRAPOLATION ASPC
      EXTRAPOLATION_ORDER 2
    &END QS
    &MGRID
      NGRIDS 4
      CUTOFF 400
      REL_CUTOFF 60
    &END MGRID
    &XC
      &XC_FUNCTIONAL PBE
      &END XC_FUNCTIONAL
      &VDW_POTENTIAL
        POTENTIAL_TYPE PAIR_POTENTIAL
        &PAIR_POTENTIAL
          TYPE DFTD3
          PARAMETER_FILE_NAME dftd3.dat
          REFERENCE_FUNCTIONAL PBE
        &END PAIR_POTENTIAL
      &END VDW_POTENTIAL
    &END XC
    &PRINT
      &V_HARTREE_CUBE
        ADD_LAST NUMERIC
        COMMON_ITERATION_LEVELS 0
        APPEND .TRUE.
        &EACH
          GEO_OPT 0
        &END EACH
      &END V_HARTREE_CUBE
      &PDOS
        ADD_LAST NUMERIC
        COMMON_ITERATION_LEVELS 0
        COMPONENTS 
        APPEND .TRUE.
        NLUMO -1
        &EACH
          GEO_OPT 0
        &END EACH
      &END PDOS
    &END PRINT
  &END DFT
  &SUBSYS
